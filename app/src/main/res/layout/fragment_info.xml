<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".tats.Info">

    <ImageView
        android:id="@+id/imageView23"
        android:layout_width="272dp"
        android:layout_height="301dp"
        android:src="@drawable/cam_icon"
        tools:layout_editor_absoluteX="69dp"
        tools:layout_editor_absoluteY="83dp" />

    <TextView
        android:id="@+id/textView27"
        android:layout_width="209dp"
        android:layout_height="46dp"
        android:text="Add photo"
        android:textColor="#E91E63"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="241dp"
        tools:layout_editor_absoluteY="342dp" />

    <EditText
        android:id="@+id/editTextTextPersonName"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Enter your weight"
        android:textSize="@dimen/_20sdp"

        android:inputType="textPersonName"
        android:textColorHint="@color/hint_color1"
        tools:layout_editor_absoluteX="69dp"
        tools:layout_editor_absoluteY="446dp" />

    <EditText
        android:id="@+id/editTextTextPersonName2"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Enter your height"
        android:inputType="textPersonName"
        android:textColorHint="@color/hint_color1"
        android:textSize="@dimen/_20sdp"
        tools:layout_editor_absoluteX="69dp"
        tools:layout_editor_absoluteY="522dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/btn_shape_1"
        android:text="Done"
        android:textColor="@color/white"
        tools:layout_editor_absoluteX="255dp"
        tools:layout_editor_absoluteY="640dp" />
</androidx.constraintlayout.widget.ConstraintLayout>